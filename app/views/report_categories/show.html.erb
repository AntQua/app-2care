<div class="page-title">
  <p><%= @report_category.category.name %></p>
</div>

<%= simple_form_for @report_category do |f| %>
   <div class="mb-3 text-white ">
    <%= f.input :result, collection: Category.find(@category.id).options.pluck(:name)  %>
  </div>

  <div class="form-actions">
    <%= f.submit %>
  </div>
<% end %>
